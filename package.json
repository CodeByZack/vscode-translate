{
  "name": "empty-translate",
  "displayName": "空山翻译",
  "description": "选中单词，悬停1秒，翻译就出来了。。。",
  "repository": "https://github.com/CodeByZack/vscode-translate",
  "version": "0.0.5",
  "publisher": "zackdk",
  "engines": {
    "vscode": "^1.39.0"
  },
  "scripts": {
    "pr": "prettier --write './src/**/*.js'"
  },
  "categories": [
    "Other"
  ],
  "icon": "image/icon.jpg",
  "activationEvents": [
    "onCommand:extension.translate",
    "onLanguage:javascript",
    "onLanguage:typescript"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.translate",
        "title": "翻译"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorFocus",
          "command": "extension.translate",
          "group": "navigation"
        }
      ]
    }
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.39.0",
    "eslint": "^5.13.0",
    "glob": "^7.1.4",
    "mocha": "^10.2.0",
    "prettier": "1.18.2",
    "typescript": "^3.3.1",
    "vscode-test": "^1.2.0"
  },
  "dependencies": {
    "request": "^2.88.0",
    "request-promise": "^4.2.4"
  }
}
